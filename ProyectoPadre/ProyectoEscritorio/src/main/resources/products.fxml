<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<Pane fx:id="panelPrincipal" prefHeight="495.0" prefWidth="649.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.gui.controlador.productsController">
	<children>
		<Pane fx:id="panelTabla" layoutX="7.0" layoutY="196.0" prefHeight="293.0" prefWidth="636.0" style="-fx-background-color: #D7DBDD;">
			<children>
				<GridPane layoutX="14.0" layoutY="7.0" prefHeight="279.0" prefWidth="604.0">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints maxHeight="64.0" minHeight="6.0" prefHeight="13.0" vgrow="SOMETIMES" />
						<RowConstraints maxHeight="135.0" minHeight="7.0" prefHeight="43.0" vgrow="SOMETIMES" />
						<RowConstraints maxHeight="223.0" minHeight="10.0" prefHeight="223.0" vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<HBox alignment="CENTER" prefHeight="82.0" prefWidth="476.0" GridPane.rowIndex="1">
							<children>
								<Button fx:id="buttonSearch" alignment="CENTER" mnemonicParsing="false" onAction="#action_search_product" prefHeight="29.0" prefWidth="83.0" style="-fx-background-color: #154360;" text="Search" textFill="WHITE">
									<HBox.margin>
										<Insets />
									</HBox.margin>
									<graphic>
										<FontAwesomeIcon glyphName="SEARCH" />
									</graphic>
									<cursor>
										<Cursor fx:constant="OPEN_HAND" />
									</cursor>
								</Button>
								<JFXTextField fx:id="textSearch" alignment="CENTER" prefHeight="26.0" prefWidth="388.0" promptText="Insert ID for search">
									<HBox.margin>
										<Insets left="30.0" />
									</HBox.margin>
									<padding>
										<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
									</padding>
								</JFXTextField>
							</children>
							<GridPane.margin>
								<Insets />
							</GridPane.margin>
							<padding>
								<Insets bottom="5.0" right="5.0" top="5.0" />
							</padding>
						</HBox>
						<TableView fx:id="tableProducts" prefHeight="200.0" prefWidth="603.0" stylesheets="@style.css" GridPane.rowIndex="2">
							<columns>
								<TableColumn prefWidth="90.0" text="Identifier" />
								<TableColumn prefWidth="136.0" text="Full name" />
								<TableColumn prefWidth="75.0" text="Adress" />
								<TableColumn prefWidth="75.0" text="Cell phone" />
								<TableColumn prefWidth="75.0" text="Email adress" />
							</columns>
							<columnResizePolicy>
								<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
							</columnResizePolicy>
						</TableView>
					</children>
				</GridPane>
			</children>
			<effect>
				<DropShadow />
			</effect>
		</Pane>
		<Pane fx:id="panelSuperior" layoutX="7.0" layoutY="7.0" prefHeight="180.0" prefWidth="636.0" style="-fx-background-color: yellow;" />
	</children>
</Pane>
